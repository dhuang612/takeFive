{
  "manifest_version": 2,
  "name": "takeFive",
  "version": "1.0",

  "description": "set timers that alert you when, you should take a moment to stand up / drink water or look away from the screen",
  "icons": {
    "48": "icons/palm.png"
  },
  "permissions": [
    "alarms",
    "notifications",
    "storage"
  ],
  "sidebar_action": {
    "default_icon": "icons/palm.png",
    "default_title": "takeFive",
    "default_panel": "popup/main.html",
    "open_at_install":true
  }

}
